#!/usr/bin/env bash

## If there is a docker-${COMMAND}, try that.
## Otherwise, assume it is a docker builtin and use the real docker.

COMMAND=$1
shift

SUB_COMMAND=$(type -path docker-${COMMAND})
if [ -n "$SUB_COMMAND" -a -x "$SUB_COMMAND" ]; then
    exec $SUB_COMMAND "$@"
else
    command -p docker $COMMAND "$@"
fi
